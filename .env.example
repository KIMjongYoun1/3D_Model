# [Database]
DATABASE_URL=jdbc:postgresql://localhost:5432/postgres
DB_HOST=localhost
DB_PORT=5432
DB_NAME=postgres
DB_USER=model_dev
DB_PASSWORD=dev1234

# [Backend URLs]
