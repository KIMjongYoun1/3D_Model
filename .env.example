JWT_EXPIRE_MINUTES=60
GEMINI_API_KEY=your_gemini_api_key_here

# [Social Auth - Naver]
NAVER_CLIENT_ID=your_naver_client_id
NEXT_PUBLIC_NAVER_CLIENT_ID=your_naver_client_id
